{
    "manifest_version": 2,
    "name": "basketty",
    "version": "0.1.3",
    "description": "basketty",
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["https://*/*"]
    }],
    "background": {
        "persistent": false,
        "scripts": ["bg.js"]
      },
    "permissions": [ "cookies", "http://*/*", "https://*/*" ]
}
